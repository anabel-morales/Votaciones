        <servlet-name>ControladorResultadoElectos</servlet-name>
        <url-pattern>/ControladorResultadoElectos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorEscrutinio</servlet-name>
        <url-pattern>/ControladorEscrutinio</url-pattern>
