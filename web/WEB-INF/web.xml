    <servlet-mapping>
        <servlet-name>ControladorInsertarVotante</servlet-name>
        <url-pattern>/ControladorInsertarVotante</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorLogin</servlet-name>
        <url-pattern>/ControladorLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorBajaVotante</servlet-name>
        <url-pattern>/ControladorBajaVotante</url-pattern>
